                );
        }


        var chart = new CanvasJS.Chart("chartContainerForPresentSO", {
            axisX: {
                labelMaxWidth: 100,
                labelWrap: true,   // change it to false
                labelFontSize: 9,
                interval: 1
            },
            animationEnabled: true,
            theme: "light1",
            title: {
                text: "Available Doctor Today"
            },
            axisX:{
                titleFontSize: 20,
              //  titleFontColor: "black",
                // title: "Fruit",
                labelFontSize: 9,
            },
            data: [{
                type: "column",
                indexLabel: "{y}",
                startAngle: 200,
                axisY:{
                    minimum: 1,
                },
                //yValueFormatString: "##0.00\"%\"",
               // indexLabel: "{label} {y}",
                dataPoints: ColumnChartData
            }]
        });
        chart.render();


        var result = @Html.Raw(ViewBag.DataPoints3);
        debugger;

        var ColumnChartData =[];
        for(var i = 0; i < result.length; i++){
            ColumnChartData.push(
                {y:result[i].Visits, label:result[i].SaleOfficerName}
