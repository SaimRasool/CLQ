                //yValueFormatString: "##0.00\"%\"",
                // indexLabel: "{label} {y}",
                dataPoints: ColumnChartData
            }]
        });
        chart.render();



    var result = @Html.Raw(ViewBag.DataPoints);
    debugger;

    var ColumnChartData =[];
    for(var i = 0; i < result.length; i++){
        ColumnChartData.push(
            {y:result[i].Visits, label:result[i].Name}
            );
    }


    var chart = new CanvasJS.Chart("chartContainer", {
        axisX: {
            labelMaxWidth: 100,
            labelWrap: true,   // change it to false
            interval: 1
        },
    animationEnabled: true,
    theme: "light1",
	title: {
		text: "Total Appointment This Month"
	},  
	
	axisX:{
	    titleFontSize: 20,
	    titleFontColor: "black",
