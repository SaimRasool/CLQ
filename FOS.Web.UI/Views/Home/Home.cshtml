	data: [{

        type: "pie",
	    //type: "column",
	    indexLabel: "{y}",
		startAngle: 340,
		//yValueFormatString: "##0.00\"%\"",
	//	indexLabel: "{label} {y}",
		dataPoints: ColumnChartData
	}]
});
chart.render();


var result = @Html.Raw(ViewBag.DataPoints1);
debugger;

var pieChartData =[];
for(var i = 0; i < result.length; i++){
    pieChartData.push(
        {y:result[i].Visits, label:result[i].City}
        );
}


var Piechart = new CanvasJS.Chart("ChartContainerCitywise", {
